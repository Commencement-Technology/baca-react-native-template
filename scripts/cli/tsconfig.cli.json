{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "rootDir": ".",
    "noImplicitAny": false,
    "outDir": "build",
    "strict": true,
    "lib": ["ESNext"],
    "target": "es6",
    "module": "commonjs",
    "sourceMap": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true
  }
}
